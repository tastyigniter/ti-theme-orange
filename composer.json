{
  "name": "tastyigniter/ti-theme-orange",
  "type": "tastyigniter-package",
  "description": "Free Modern, Responsive and Clean TastyIgniter Theme based on Livewire and Bootstrap.",
  "license": "MIT",
  "authors": [
    {
      "name": "TastyIgniter Dev Team"
    }
  ],
  "keywords": [
    "tastyigniter",
    "theme",
    "orange"
  ],
  "require": {
    "livewire/livewire": "^v3.0.0",
    "spatie/laravel-google-fonts": "^1.2",
    "tastyigniter/core": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-cart": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-coupons": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-frontend": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-local": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-pages": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-payregister": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-reservation": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-socialite": "^v4.0@beta || ^v4.0@dev",
    "tastyigniter/ti-ext-user": "^v4.0@beta || ^v4.0@dev"
  },
  "require-dev": {
    "larastan/larastan": "^2.4.0",
    "laravel/pint": "^1.2",
    "pestphp/pest-plugin-laravel": "^3.0",
    "rector/rector": "^1.2",
    "sampoyigi/testbench": "dev-main as 1.0"
  },
  "autoload": {
    "psr-4": {
      "Igniter\\Orange\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Igniter\\Orange\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Igniter\\Orange\\ServiceProvider"
      ]
    },
    "tastyigniter-theme": {
      "code": "igniter-orange",
      "name": "Orange Theme",
      "locked": true,
      "source-path": "/resources/views",
      "meta-path": "/resources/meta",
      "publish-paths": [
        "/public"
      ],
      "require": {
        "igniter.broadcast": "*",
        "igniter.cart": "*",
        "igniter.coupons": "*",
        "igniter.frontend": "*",
        "igniter.local": "*",
        "igniter.pages": "*",
        "igniter.payregister": "*",
        "igniter.reservation": "*",
        "igniter.socialite": "*",
        "igniter.translate": "*",
        "igniter.user": "*"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage --exactly=100 --compact",
    "type-coverage": "vendor/bin/pest --type-coverage --min=100",
    "format": "vendor/bin/pint",
    "refactor": "vendor/bin/rector process --dry-run",
    "static": "vendor/bin/phpstan analyse --ansi --memory-limit 1056M",
    "test-suite": [
      "@refactor",
      "@static",
      "@test-coverage"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "composer/installers": true
    }
  },
  "minimum-stability": "dev"
}
